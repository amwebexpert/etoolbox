{
  "name": "web-toolbox",
  "version": "2.1.4",
  "author": {
    "name": "AndrÃ© Masson",
    "email": "amwebexpert@gmail.com",
    "url": "https://www.linkedin.com/in/amwebexpert/"
  },
  "description": "Collection of web developer utilities packaged as a desktop app",
  "homepage": ".",
  "main": "build/electron/main.js",
  "private": false,
  "scripts": {
    "release": "standard-version",
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts --openssl-legacy-provider start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "postinstall": "patch-package",
    "postinstall:electron:compatible": "patch-package && electron-builder install-app-deps",
    "electron:dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:windev": "concurrently \"SET BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder",
    "electron:build:linux": "yarn build && tsc -p electron && electron-builder -l",
    "electron:build:all": "yarn build && tsc -p electron && electron-builder -mwl",
    "eject": "react-scripts eject",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run",
    "e2e:run:spec:home": "cypress run --spec cypress/e2e/home.cy.ts",
    "e2e:run:spec:url": "cypress run --spec cypress/e2e/url.cy.ts",
    "e2e:run:ci": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && yarn e2e:run\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "pretty": "prettier --write \"./**/*.{ts,tsx}\"",
    "check-licences": "license-checker --direct --production --exclude MIT,Apache-2.0,BSD-2-Clause,BSD-3-Clause,ISC,CC0-1.0"
  },
  "build": {
    "appId": "com.amwebexpert.etoolbox",
    "productName": "Web Toolbox",
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns",
      "category": "public.app-category.developer-tools",
      "type": "distribution",
      "hardenedRuntime": true
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.106",
    "@mui/material": "^5.10.12",
    "@mui/styles": "^5.10.10",
    "@mui/system": "^5.10.12",
    "@mui/x-date-pickers": "^5.0.7",
    "axios": "^1.1.3",
    "buffer": "^6.0.3",
    "connected-react-router": "^6.9.3",
    "copy-to-clipboard": "^3.3.2",
    "curlconverter": "^4.3.0",
    "date-fns": "^2.29.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "file-saver": "^2.0.5",
    "js-beautify": "^1.14.7",
    "jwt-decode": "^3.1.2",
    "mime-db": "^1.52.0",
    "papaparse": "^5.3.2",
    "path-browserify": "^1.0.1",
    "pretty-bytes": "^6.0.0",
    "preval.macro": "^5.0.0",
    "process": "^0.11.10",
    "qrcode": "^1.5.1",
    "quicktype-core": "^6.1.0",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.18.0",
    "react-hook-form": "^7.39.1",
    "react-html-parser": "^2.0.2",
    "react-loading-overlay": "^1.0.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.6",
    "react-syntax-highlighter": "^15.5.0",
    "react-web-share": "^2.0.2",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^2.0.0",
    "redux-saga": "^1.2.1",
    "regex-parser": "^2.2.11",
    "styled-components": "^5.3.6",
    "tesseract.js": "^3.0.3",
    "typescript": "^4.8.4",
    "url": "^0.11.0",
    "use-debounce": "^8.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.2.2",
    "@types/js-beautify": "^1.13.3",
    "@types/mime-db": "^1.43.1",
    "@types/node": "^18.11.9",
    "@types/papaparse": "^5.3.5",
    "@types/preval.macro": "^3.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.0.25",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.8",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-html-parser": "^2.0.2",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/redux-actions": "^2.6.2",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "concurrently": "^7.5.0",
    "cypress": "^10.11.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.2.3",
    "electron": "^21.2.2",
    "electron-builder": "^23.6.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "patch-package": "^6.5.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "redux-devtools-extension": "^2.13.9",
    "standard-version": "^9.5.0",
    "wait-on": "^6.0.1"
  }
}
