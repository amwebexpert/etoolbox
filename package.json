{
  "name": "web-toolbox",
  "version": "4.1.0",
  "author": "amwebexpert@gmail.com",
  "repository": "https://github.com/amwebexpert/etoolbox",
  "contributors": [
    {
      "name": "Andr√© Masson",
      "email": "amwebexpert@gmail.com",
      "url": "https://www.linkedin.com/in/amwebexpert/"
    },
    {
      "name": "Anthony Buchholz",
      "email": "anthony.buchholz@gmail.com",
      "url": "https://yodigi7.github.io/"
    }
  ],
  "description": "Collection of web development utilities. Like a Swiss knife for web developers.",
  "homepage": ".",
  "type": "module",
  "scripts": {
    "clean:node": "npx rimraf node_modules yarn.lock",
    "start": "yarn dev",
    "dev": "vite",
    "typecheck": "npx tsc --noEmit --project tsconfig.app.json",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 rimraf dist && yarn generate:version && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "githubpage:rebuild": "rimraf dist && npm run build && rimraf docs && shx mv dist docs",
    "generate:version": "node scripts/generate-version",
    "generate:api:client": "npx @hey-api/openapi-ts -i ./public/openapi/openapi.json -o ./src/api/generated --client @hey-api/client-axios",
    "test": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@lichens-innovation/ts-common": "^1.5.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.144.0",
    "@types/three": "^0.182.0",
    "@uidotdev/usehooks": "^2.4.1",
    "antd": "^6.1.3",
    "antd-style": "^4.1.0",
    "axios": "^1.13.2",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "httpsnippet": "^3.0.1",
    "immer": "^11.1.3",
    "js-beautify": "^1.15.4",
    "jsonrepair": "^3.13.1",
    "jsqr": "^1.4.0",
    "jwt-decode": "^4.0.0",
    "markdown-it": "^14.1.0",
    "mime-db": "^1.54.0",
    "papaparse": "^5.5.3",
    "parse-curl": "^0.2.6",
    "path-browserify": "^1.0.1",
    "pretty-bytes": "^7.1.0",
    "process": "^0.11.10",
    "qrcode": "^1.5.4",
    "querystring-es3": "^0.2.1",
    "quicktype-core": "^23.2.6",
    "re-resizable": "^6.11.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.69.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "reconnecting-websocket": "^4.4.0",
    "sonner": "^2.0.7",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "tesseract.js": "^7.0.0",
    "three": "^0.182.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@eslint/js": "^9.39.1",
    "@types/js-beautify": "^1.14.3",
    "@types/markdown-it": "^14.1.2",
    "@types/mime-db": "^1.43.6",
    "@types/node": "^24.10.1",
    "@types/papaparse": "^5.5.2",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.7",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "commitlint": "^20.2.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.1",
    "patch-package": "^8.0.1",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "shx": "^0.3.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,json,md,html}": "prettier --write"
  }
}
